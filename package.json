{
  "name": "piano-chord-app",
  "version": "1.0.0",
  "description": "A simple piano chord learning app",
  "homepage": "https://ItsNoahJ.github.io/Piano-App",
  "scripts": {
    "start": "parcel index.html",
    "build": "parcel build index.html",
    "predeploy": "rimraf dist && parcel build index.html --public-url /Piano-App/ && copyfiles -u 1 public/404.html dist",
    "deploy": "gh-pages -d dist",
    "deploy:wizard": "node deploy.js"
  },
  "keywords": [
    "react",
    "typescript",
    "piano",
    "tonejs"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tone": "^15.1.22"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.6",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "autoprefixer": "^10.4.16",
    "copyfiles": "^2.4.1",
    "gh-pages": "^6.1.1",
    "parcel": "^2.11.0",
    "postcss": "^8.4.32",
    "rimraf": "^6.0.1",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.8.3"
  }
}
